{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "project"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../../ng",
      "assets": [
        "template",
        "favicon.ico"
      ],
      "index": "index.htm",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/ng2-tree/styles.css",
        "../node_modules/rickshaw/rickshaw.min.css",
        "../src/template/css/rickshaw/extensions.css",
        "../node_modules/ng-selectize/selectize/selectize.css",
		"../node_modules/selectize/dist/css/selectize.default.css"
      ],
      "scripts": [
      	"../node_modules/d3/d3.min.js",
      	"../node_modules/rickshaw/rickshaw.min.js",
      	"../src/template/framework/rickshaw/extensions.js",
      	"../src/template/framework/rickshaw/Rickshaw.Graph.RangeSlider.MultiPreview.min.js",
      	"../src/template/framework/rickshaw/Rickshaw.Graph.SmootherPlus.min.js",
		"../node_modules/ng-selectize/selectize/selectize.standalone.js",
		"../node_modules/lzma/src/lzma_worker-min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
